{
	"name": "de-swebhosting-typo3-extension/fal-dummy",
	"description": "FAL dummy image driver",
	"type": "typo3-cms-extension",
	"license": "GPL-3.0-or-later",
	"require": {
		"php": ">=5.6.0",
		"typo3/cms-core": "^8.7.55|^9.5.44"
	},
	"replace": {
		"typo3-ter/fal-dummy": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Tx\\FalDummy\\": "Classes/"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
